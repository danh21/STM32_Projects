/* LIBRARIES */
#include <stdint.h>
#include <stm32f407xx.h>
#include "Timer.h"
#include "SystemClock.h"
#include "USART.h"



/* MACROES */
#define send_data USART_transmit(USART2, "danh21\n");



/* MAIN */
int main()
{
	SystemClock_config();
	Timer1_init();
	USART_init(USART2);

	while(1)
	{
		send_data;
		Timer1_delay_ms(1000);
	}
}
